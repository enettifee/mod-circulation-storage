{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Collection of checkout locks",
  "type": "object",
  "properties": {
    "checkoutLocks": {
      "description": "List of check out locks",
      "id": "checkOutLocks",
      "type": "array",
      "items": {
        "type": "object",
        "$ref": "checkout-lock.json"
      }
    },
    "totalRecords": {
      "type": "integer"
    }
  },
  "required": [
    "checkOutLocks",
    "totalRecords"
  ]
}
